{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "projects": {
    "anime-gallery-frontend": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": {
              "base": "./anime-gallery-electron/app/views"
            },
            "index": "src/index.html",
            "polyfills": ["zone.js"],
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": ["src/assets"],
            "styles": [
              "video.js/dist/video-js.css",
              "src/assets/scss/videojs-theme-1.scss",
              "@videojs/themes/dist/forest/index.css",
              "@videojs/themes/dist/city/index.css",
              "@videojs/themes/dist/fantasy/index.css",
              "@videojs/themes/dist/sea/index.css",
              "videojs-playlist-ui/dist/videojs-playlist-ui.css",
              "src/mat-theme.scss",
              "src/fonts/fonts.scss",
              "src/styles.scss"
            ],
            "scripts": ["src/assets/js/electron-titlebar-renderer.js"],
            "allowedCommonJsDependencies": [
              "tailwindcss/colors",
              "videojs-playlist",
              "videojs-playlist-ui",
              "videojs-hotkeys",
              "@videojs/xhr",
              "global/document",
              "global/window",
              "keycode",
              "mux.js/lib/tools/parse-sidx",
              "mux.js/lib/utils/clock",
              "safe-json-parse/tuple",
              "videojs-vtt.js"
            ],
            "browser": "src/main.ts"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "anime-gallery-frontend:build:production"
            },
            "development": {
              "buildTarget": "anime-gallery-frontend:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "production": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "buildTarget": "anime-gallery-frontend:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "anime-gallery-frontend:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "anime-gallery-frontend:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:vitest",
          "options": {
            "watch": true,
            "tsConfig": "tsconfig.spec.json"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": ["src"]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "anime-gallery-frontend:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "anime-gallery-frontend:serve:production"
            }
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  },
  "schematics": {
    "@schematics/angular:component": {
      "type": "component"
    },
    "@schematics/angular:directive": {
      "type": "directive"
    },
    "@schematics/angular:service": {
      "type": "service"
    },
    "@schematics/angular:guard": {
      "typeSeparator": "."
    },
    "@schematics/angular:interceptor": {
      "typeSeparator": "."
    },
    "@schematics/angular:module": {
      "typeSeparator": "."
    },
    "@schematics/angular:pipe": {
      "typeSeparator": "."
    },
    "@schematics/angular:resolver": {
      "typeSeparator": "."
    }
  }
}
