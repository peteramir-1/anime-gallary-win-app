{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "projects": {
    "anime-gallery-frontend": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "./anime-gallery-electron/app/views",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": ["src/assets"],
            "styles": ["src/fonts/fonts.scss", "src/styles.scss"],
            "scripts": ["src/assets/js/electron-titlebar-renderer.js"],
            "allowedCommonJsDependencies": [
              "videojs-playlist",
              "videojs-playlist-ui",
              "videojs-hotkeys",
              "@videojs/xhr",
              "global/document",
              "global/window",
              "keycode",
              "mux.js/lib/tools/parse-sidx",
              "mux.js/lib/utils/clock",
              "safe-json-parse/tuple",
              "videojs-vtt.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "anime-gallery-frontend:build:production"
            },
            "development": {
              "buildTarget": "anime-gallery-frontend:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "production": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "buildTarget": "anime-gallery-frontend:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "anime-gallery-frontend:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "anime-gallery-frontend:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": ["src/assets"],
            "styles": ["src/fonts/fonts.scss", "src/styles.scss"],
            "scripts": ["src/assets/js/electron-titlebar-renderer.js"]
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": ["src"]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "anime-gallery-frontend:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "anime-gallery-frontend:serve:production"
            }
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
