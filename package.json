{
  "name": "anime_gallery",
  "version": "1.0.0",
  "description": "FrontEnd Angular application for anime-gallery-electron project",
  "scripts": {
    "frontend:start": "ng serve",
    "frontend:dev:build": "ng build --configuration development",
    "frontend:prod:build": "ng build --configuration production",
    "frontend:dev:watch": "ng build --watch --configuration development",
    "frontend:prod:watch": "ng build --watch --configuration production",
    "frontend:test": "ng test",
    "frontend:lint": "tsc --noEmit && eslint . --ext js,ts,json --quiet --fix",
    "frontend:codegen": "graphql-codegen --config codegen.ts",
    "backend:start:electron": "npm run frontend:dev:build && cd ./anime-gallery-electron && npm run start:electron",
    "backend:start:apollo": "npm run frontend:dev:build && cd ./anime-gallery-electron && npm run start:apollo",
    "backend:test": "cd ./anime-gallery-electron && npm run publish",
    "compile": "cd ./anime-gallery-electron && npm run compile",
    "package": "cd ./anime-gallery-electron && npm run package",
    "make": "cd ./anime-gallery-electron && npm run make",
    "publish": "cd ./anime-gallery-electron && npm run publish"
  },
  "keywords": [],
  "author": {
    "name": "Peter A. Labib",
    "email": "peteramir5522@outlook.com"
  },
  "license": "MIT",
  "dependencies": {
