    "@angular/platform-browser-dynamic": "^19.2.18",
    "@angular/router": "^19.2.18",
    "@apollo/client": "^4.0.1",
    "@ng-icons/core": "^22.4.0",
    "@ng-icons/heroicons": "^25.6.1",
    "@ng-icons/jam-icons": "^25.6.1",
    "@ng-icons/lucide": "^25.6.1",
    "@ng-icons/material-file-icons": "^28.1.0",
    "@ng-icons/material-icons": "^25.6.1",
    "@typescript-eslint/types": "^8.53.0",
    "@typescript-eslint/utils": "^8.53.0",
    "@videojs/themes": "^1.0.1",
    "apollo-angular": "^13.0.0",
